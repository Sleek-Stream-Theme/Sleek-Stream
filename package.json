{
  "name": "sleek-stream",
  "version": "1.0.0",
  "description": "Custom Hugo theme with Tailwind CSS, SCSS, Summery Panel, Admin Panel, and Light/Dark theme support.",
  "scripts": {
    "npm-check": "npm-check -u",
    "dev": "hugo server --buildDrafts --buildFuture --buildExpired --gc --logLevel info --watch",
    "prod": "hugo server --buildDrafts --buildFuture --buildExpired --gc --logLevel info --watch  -e production",
    "build": "hugo --gc --logLevel info --cleanDestinationDir --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sleek-Stream-Theme/Sleek-Stream.git"
  },
  "keywords": [
    "Hugo",
    "Custom Theme",
    "Custom",
    "Theme",
    "TailwindCss",
    "Scss",
    "Light Mode",
    "Dark Mode",
    "Light/Dark Mode",
    "Admin Panel",
    "Summery Panel",
    "Responsive",
    "Customizeable",
    "Template"
  ],
  "author": "Armoghan-ul-Mohmin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sleek-Stream-Theme/Sleek-Stream/issues"
  },
  "homepage": "https://github.com/Sleek-Stream-Theme/Sleek-Stream#readme",
  "devDependencies": {
    "npm-check": "^6.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-go-template": "^0.0.15",
    "prettier-plugin-tailwindcss": "^0.5.10"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.0",
    "flowbite": "^2.2.1",
    "preline": "^2.0.3"
  }
}
